#include <stdio.h>
#include "lists.h"
#include <stdlib.h>

/**
 *print_list - prints all elements in a linked list
 *@h: pointer to the head node of the list
 *Return: number of nodes
 */

size_t print_list(const list_t *h)
{
	int a = 0;

	if (h == NULL)
	{
		return (0);
	}
	while (h != NULL)
	{
		if (h->str == NULL)
		{
			printf("[0] (nil)");
		}
		else
		{
			printf("[%u] %s\n", h->len, h->str);
		}
		a++;
		h = h->next;
	}
	return (a);
}
